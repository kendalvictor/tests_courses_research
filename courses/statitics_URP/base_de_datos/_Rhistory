foundridge<-cv.glmnet(predictores,precio_promedio,alpha=0,nfolds=5)
foundridge<-cv.glmnet(predictores.train,precio_promedio,alpha=0,nfolds=5)
foundridge<-cv.glmnet(predictores.train,precio.train,alpha=0,nfolds=5)
foundridge.train<-cv.glmnet(predictores.train,precio.train,alpha=0,nfolds=5)
plot(foundridge)
foundridge$lambda.1se
foundridge$lambda.min
coef(fitridge,s=foundridge$lambda.1se)
coef(fitridge,s=foundridge$lambda.min)
fitlasso<-glmnet(predictores,precio,alpha = 1)
fitlasso<-glmnet(predictores.train,precio.train,alpha = 1)
